		Форма, 
		Элементы, 
		"КСЭ_НомерДокумента", 
		"КСЭ_НомерДокумента", 
		ГруппаКСЭ_Строка,
		ДополнительныеПараметры
	);
	ЭлементНомерДокументаКСЭ.ЦветФона = ЦветФона;
	
	ЭлементСтатусДокументаКСЭ = ts_ЕвропейскаяЗонаУправлениеЭлементамиСервер.ДобавитьЭлементНаФорму(
		Форма, 
		Элементы, 
		"КСЭ_СтатусДокумента", 
		"КСЭ_СтатусДокумента", 
		ГруппаКСЭ_Строка,
		ДополнительныеПараметры
	);
	ЭлементСтатусДокументаКСЭ.ЦветФона = ЦветФона;
	
	ЭлементСтатусДокументаКСЭ = ts_ЕвропейскаяЗонаУправлениеЭлементамиСервер.ДобавитьЭлементНаФорму(
		Форма, 
		Элементы, 
		"КСЭ_ДатаЗабора", 
		"КСЭ_ДатаЗабора", 
		ГруппаКСЭ,
		ДополнительныеПараметры
	);	
	
КонецПроцедуры

&НаСервере
Процедура ЗаписатьДанныеКСЭ()

	Набор = РегистрыСведений.РеестрКСЭ.СоздатьНаборЗаписей();
	Набор.Отбор.Документ.Установить(Объект.Ссылка);	
	Набор.Прочитать();
	
	Если Набор.Количество() = 0 Тогда
		Запись = Набор.Добавить();
	Иначе
		Запись = Набор[0];	
	КонецЕсли;
	
	Запись.Документ = Объект.Ссылка;
	Запись.ТипДокументаКСЭ = Перечисления.ксэ_ТипыДокументов.ЗаявкаНаПриход;
	Запись.НомерДокумента = ЭтаФорма.КСЭ_НомерДокумента;
	Запись.СтатусДоставки = ЭтаФорма.КСЭ_СтатусДокумента;
	Запись.ДатаЗабора = ЭтаФорма.КСЭ_ДатаЗабора;
	
	Набор.Записать();
	
КонецПроцедуры

&НаСервере
Процедура ПрочитатьДанныеКСЭ()
	
	Запрос = Новый Запрос;
	Запрос.Текст = "ВЫБРАТЬ
	               |	РеестрКСЭ.Документ,
	               |	РеестрКСЭ.ТипДокументаКСЭ,
	               |	РеестрКСЭ.НомерДокумента,
	               |	РеестрКСЭ.СтатусДоставки,
	               |	РеестрКСЭ.ДатаЗабора
				   |ИЗ
	               |	РегистрСведений.РеестрКСЭ КАК РеестрКСЭ
	               |ГДЕ
	               |	РеестрКСЭ.Документ = &Документ";
	Запрос.УстановитьПараметр("Документ", Объект.Ссылка);
	
	Выборка = Запрос.Выполнить().Выбрать();
	
	Если Выборка.Следующий() Тогда
		
		ЭтаФорма.КСЭ_НомерДокумента = Выборка.НомерДокумента;
		ЭтаФорма.КСЭ_СтатусДокумента = Выборка.СтатусДоставки;
		ЭтаФорма.КСЭ_ДатаЗабора = Выборка.ДатаЗабора;
		
	КонецЕсли;
	
КонецПроцедуры

&НаСервере
Процедура ТоварыСтавкаНДСПриИзмененииНаСервере(ИдСтроки)
	
	ts_ЕвропейскаяЗонаУправлениеЭлементамиСервер.ТоварыСтавкаНДСПриИзменении(Объект.Товары.НайтиПоИдентификатору(ИдСтроки));
	
КонецПроцедуры

&НаСервере
Процедура Европа_ПересчитатьДатуПлатежа() Экспорт
	
	Объект.ДатаПлатежа = ?(ЗначениеЗаполнено(Объект.Дата), Объект.Дата, ТекущаяДата()) + (Объект.Договор.КоличествоДнейОтсрочки * 24 * 60 * 60);
	
КонецПроцедуры

